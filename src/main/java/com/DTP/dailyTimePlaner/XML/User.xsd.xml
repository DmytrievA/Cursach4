<user:user id="1" role="user" xmlns:user="http://www.itroi.org/user" xmlns:group="http://www.itroi.org/group">
  <user:name>string</user:name>
  <!--Optional:-->
  <user:surname>string</user:surname>
  <user:email>string</user:email>
  <user:login>string</user:login>
  <user:password>stringst</user:password>
  <!--Optional:-->
  <user:gender>string</user:gender>
  <user:memberOfGroups>
    <!--0 to 10 repetitions:-->

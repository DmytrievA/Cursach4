      <user:group id="1">
        <group:name>string</group:name>
      </user:group>
      <user:groupRole id="1">
        <user:groupRoleName>OneToSelf</user:groupRoleName>
      </user:groupRole>
    </user:memberOfGroup>
  </user:memberOfGroups>
</user:user>